{{#foreach navigation}}
	{{#if url == "/?blog_page=blog"}}
      <li class="{{slug}}{{#if current}} active{{/if}}"> <a href="/?blog_page=blog"> {{label}} </a> </li>
   {{else}}
      <li class="{{slug}}{{#if current}} active{{/if}}"> <a href="{{url absolute="true"}}"> {{label}} </a> </li>
   {{/if}}
{{/foreach}}
